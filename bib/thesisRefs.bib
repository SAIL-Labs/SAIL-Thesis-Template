@proceeding{Hill:2012,
	Author = {Hill, Gary J. and Tuttle, Sarah E. and Lee, Hanshin and Vattiat, Brian L. and Cornell, Mark E. and DePoy, D. L. and Drory, Niv and Fabricius, Maximilian H. and Kelz, Andreas and Marshall, J. L. and Murphy, J. D. and Prochaska, Travis and Allen, Richard D. and Bender, Ralf and Blanc, Guillermo and Chonis, Taylor and Dalton, Gavin and Gebhardt, Karl and Good, John and Haynes, Dionne and Jahn, Thomas and MacQueen, Phillip J. and Rafal, M. D. and Roth, M. M. and Savage, R. D. and Snigula, Jan},
	Date-Added = {2015-01-26 03:57:05 +0000},
	Date-Modified = {2015-01-26 03:57:12 +0000},
	Doi = {10.1117/12.925434},
	Journal = {Proc. SPIE},
	Pages = {84460N-84460N-20},
	Title = {VIRUS: production of a massively replicated 33k fiber integral field spectrograph for the upgraded Hobby-Eberly Telescope},
	Url = {http://dx.doi.org/10.1117/12.925434},
	Volume = {8446},
	Year = {2012},
	Abstract = {
The Visible Integral-field Replicable Unit Spectrograph (VIRUS) consists of a baseline build of 150 identical
spectrographs (arrayed as 75 units, each with a pair of spectrographs) fed by 33,600 fibers, each 1.5 arcsec diameter,
deployed over the 22 arcminute field of the upgraded 10 m Hobby-Eberly Telescope (HET). The goal is to deploy 82
units. VIRUS has a fixed bandpass of 350-550 nm and resolving power R~700. VIRUS is the first example of
industrial-scale replication applied to optical astronomy and is capable of spectral surveys of large areas of sky. This
approach, in which a relatively simple, inexpensive, unit spectrograph is copied in large numbers, offers significant
savings of engineering effort, cost, and schedule when compared to traditional instruments.
The main motivator for VIRUS is to map the evolution of dark energy for the Hobby-Eberly Telescope Dark Energy
Experiment (HETDEX) using 0.8M Lyman-α emitting galaxies as tracers. The full VIRUS array is due to be deployed
by early 2014 and will provide a powerful new facility instrument for the HET, well suited to the survey niche of the
telescope. VIRUS and HET will open up wide-field surveys of the emission-line universe for the first time. We present
the production design and current status of VIRUS.
},
	Bdsk-Url-1 = {http://dx.doi.org/10.1117/12.925434}}

@proceeding{Bernstein:2014a,
	Author = {Bernstein, Rebecca A. and McCarthy, Patrick J. and Raybould, Keith and Bigelow, Bruce C. and Bouchez, Atonin H. and Filgueira, Jos{\'e} M. and Jacoby, George and Johns, Matt and Sawyer, David and Shectman, Stephen and Sheehan, Michael},
	Date-Added = {2015-01-26 03:57:05 +0000},
	Date-Modified = {2015-01-26 03:57:12 +0000},
	Doi = {10.1117/12.2055282},
	Journal = {Proc. SPIE},
	Pages = {91451C-91451C-16},
	Title = {Overview and status of the Giant Magellan Telescope project},
	Url = {http://dx.doi.org/10.1117/12.2055282},
	Volume = {9145},
	Year = {2014},
	Abstract = {
The Giant Magellan Telescope (GMT) is a 25.4-m diameter, optical/infrared telescope that is being built by an international consortium of universities and research institutions as one of the next generation of Extremely Large Telescopes. The primary mirror of GMT consists of seven 8.4 m borosilicate honeycomb mirror segments that are optically conjugate to seven corresponding segments in the Gregorian secondary mirror. Fabrication is complete for one primary mirror segment and is underway for the next two. The final focal ratio of the telescope is f/8.2, so that the focal plane has an image scale of 1.02 arcsec/mm. GMT will be commissioned using a fast-steering secondary mirror assembly comprised of conventional, rigid segments to provide seeing-limited observations. A secondary mirror with fully adaptive segments will be used in standard operation to additionally enable ground-layer and diffraction-limited adaptive optics. In the seeing limited mode, GMT will provide a 10 arcmin field of view without field correction. A 20 arcmin field of view will be obtained using a wide-field corrector and atmospheric dispersion compensator. The project has recently completed a series of sub-system and system-level preliminary design reviews and is currently preparing to move into the construction phase. This paper summarizes the technical development of the GMT sub-systems and the current status of the GMT project.
},
	Bdsk-Url-1 = {http://dx.doi.org/10.1117/12.2055282}}

